<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="mill.database.table.UserPassword" table="user_password">
    <id column="id" name="id" type="integer">
      <generator class="increment"/>
    </id>
    <many-to-one class="mill.database.table.Server" column="server" name="server" not-null="true" unique="false"/>
    <many-to-one class="mill.database.table.User" column="user" name="user" not-null="true" unique="false"/>
    <property column="password" length="64" name="password" not-null="true" type="string" unique="false"/>
    <property column="password_length" name="passwordLength" not-null="true" type="integer" unique="false"/>
  </class>
</hibernate-mapping>
